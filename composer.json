{
    "name": "yadahan/whatsapp-notification-channel",
    "description": "WhatsApp Notification Channel for Laravel.",
    "keywords": ["laravel", "notifications", "whatsapp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yaakov Dahan",
            "email": "yakidehan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Yadahan\\WhatsAppNotificationChannel\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yadahan\\WhatsAppNotificationChannel\\WhatsAppChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
